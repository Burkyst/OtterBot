<%- include('blocks/header', {bot, user, path}) %>
<div class="jumbotron">
  <div>
    <div class="row">
      <div class="col-md-6">
        <h1 draggable="true">EDM Spot</h1>
        <h4>
          <% if(bot.appInfo.description.length > 3) { %>
          <%= bot.appInfo.description %>
          <% } else { %>No description given.
          <% }%>
        </h4>
        <br>
      </div>
      <div class="col-md-6">
        <div class="row justify-content-center d-flex">
          <h5><i class="fas fa-crown"></i> Props LeaderBoard </h5>
        </div>
        <table class="table">
          <thead>
            <tr> </tr>
          </thead>
          <tbody>
            <% for (let i = 0; i < instance.length; i++) { %>
              <% console.log(instance[i]) %>
              console.log(instance[i].dataValues)
            <tr>
              <td>
                <% i %>
              </td>
              <td>
                <% instance[i].dataValues.username %>
              </td>
              <td>
                <% instance[i].dataValues.props %>
              </td>
            </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
<% include ./blocks/footer %>